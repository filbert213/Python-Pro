meme_dict = {
            "CRINGE": "Sesuatu yang sangat aneh atau memalukan",
            "LOL": "Tanggapan umum terhadap sesuatu yang lucu",
            "SUS": "Mencurigakan atau tidak dapat dipercaya",
            "NOOB": "Orang yang masih pemula dalam suatu hal",
            "GG": "Artinya permainan yang bagus (Good Game)",
            "AFK": "Pergi dari permainan untuk sementara waktu",
            "SIMP": "Orang yang terlalu mengagungkan seseorang secara berlebihan",
            "FOMO": "Takut ketinggalan tren (Fear of Missing Out)",
            "CLUTCH": "Aksi penyelamatan di saat genting",
            "NPC": "Orang yang terkesan tidak punya pendirian sendiri"
            }
word = input("Ketik kata yang tidak Kamu mengerti (gunakan huruf kapital semua!): ")
if word in meme_dict.keys():
    print("\b")
    print(meme_dict[word])
else:
    print("Maaf, kami sekarang tidak memiliki definisi untuk kata itu")
